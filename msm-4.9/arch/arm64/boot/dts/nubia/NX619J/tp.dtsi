/*****finger pinctrl start *****/
&tlmm {

	//nubia touch screen pinctrl
		nubia_synaptics_int {
			synaptics_int_active: synaptics_int_active {
				mux {
					pins = "gpio125";
					function = "gpio";
				};

				config {
					pins = "gpio125";
					drive-strength = <16>;
					bias-pull-up;
				};
			};

			synaptics_int_suspend: synaptics_int_suspend {
				mux {
					pins = "gpio125";
					function = "gpio";
				};

				config {
					pins = "gpio125";
					drive-strength = <16>;
					bias-pull-down;
				};
			};
		};
};

